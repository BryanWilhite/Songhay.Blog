{
  "name": "songhay.blog",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "asp-core:cpx": "cpx ./node_modules/normalize.css/normalize.css ./src/assets/css/",
    "asp-core:replace-in-file": "replace-in-file ~normalize.css/ ./ ./src/assets/css/styles.min.css",
    "asp-core:sass": "sass ./src/styles.scss ./src/assets/css/styles.min.css --style=compressed",
    "ng": "ng",
    "start": "ng serve --aot",
    "prebuild": "npm run asp-core:sass && npm run asp-core:cpx && npm run asp-core:replace-in-file",
    "build": "ng build --extract-css",
    "prebuild:prod": "npm run asp-core:sass && npm run asp-core:cpx && npm run asp-core:replace-in-file",
    "build:prod": "npm run build -- --prod",
    "build:ssr": "npm run build -- --app=ssr --output-hashing=media",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.1.3",
    "@angular/cdk": "^7.1.1",
    "@angular/cdk-experimental": "^7.1.1",
    "@angular/common": "7.1.3",
    "@angular/compiler": "7.1.3",
    "@angular/core": "7.1.3",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "7.1.3",
    "@angular/http": "7.1.3",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "7.1.3",
    "@angular/platform-browser-dynamic": "7.1.3",
    "@angular/platform-server": "7.1.3",
    "@angular/router": "7.1.3",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "aspnet-prerendering": "^3.0.1",
    "core-js": "^2.6.0",
    "ngx-pagination": "^3.2.1",
    "normalize.css": "^8.0.1",
    "npm": "^6.5.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.2",
    "@angular/cli": "7.1.2",
    "@angular/compiler-cli": "7.1.3",
    "@angular/language-service": "7.1.3",
    "@types/jasmine": "^3.3.1",
    "@types/jasminewd2": "^2.0.6",
    "@types/lodash": "^4.14.119",
    "@types/node": "^6.14.2",
    "codelyzer": "^4.5.0",
    "cpx": "^1.5.0",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "material-design-icons": "^3.0.1",
    "mdi": "^2.2.43",
    "protractor": "^5.4.1",
    "replace-in-file": "^3.4.2",
    "sass": "^1.15.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "3.1.6",
    "webpack": "^4.27.1",
    "webpack-dev-server": "^3.1.10"
  }
}
