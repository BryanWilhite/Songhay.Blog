{
    "name": "songhay.blog",
    "version": "2.0.0",
    "license": "MIT",
    "scripts": {
        "asp-core:cpx": "cpx ./node_modules/normalize.css/normalize.css ./src/assets/css/",
        "asp-core:replace-in-file": "replace-in-file ~normalize.css/ ./ ./src/assets/css/styles.min.css",
        "asp-core:sass": "sass ./src/styles.scss ./src/assets/css/styles.min.css --style=compressed",
        "ng": "ng",
        "start": "ng serve --extract-css",
        "prebuild": "npm run asp-core:sass && npm run asp-core:cpx && npm run asp-core:replace-in-file",
        "build": "ng build --extract-css",
        "prebuild:prod": "npm run asp-core:sass && npm run asp-core:cpx && npm run asp-core:replace-in-file",
        "build:prod": "ng build --prod --extract-css --environment=prod",
        "build:staging": "ng build --prod --extract-css",
        "build:ssr": "npm run build -- --app=ssr --output-hashing=media",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^5.2.11",
        "@angular/cdk": "^5.2.5",
        "@angular/common": "^5.2.11",
        "@angular/compiler": "^5.2.11",
        "@angular/core": "^5.2.11",
        "@angular/flex-layout": "^5.0.0-beta.15",
        "@angular/forms": "^5.2.11",
        "@angular/http": "^5.2.11",
        "@angular/material": "^5.2.5",
        "@angular/platform-browser": "^5.2.11",
        "@angular/platform-browser-dynamic": "^5.2.11",
        "@angular/platform-server": "^5.2.11",
        "@angular/router": "^5.2.11",
        "@nguniversal/module-map-ngfactory-loader": "^5.0.0",
        "aspnet-prerendering": "^3.0.1",
        "core-js": "^2.6.0",
        "ngx-pagination": "^3.2.1",
        "normalize.css": "^8.0.1",
        "npm": "^6.4.1",
        "rxjs": "^5.5.12",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular/cli": "^1.7.4",
        "@angular/compiler-cli": "^5.2.11",
        "@angular/language-service": "^5.2.11",
        "@types/jasmine": "^3.3.0",
        "@types/jasminewd2": "^2.0.6",
        "@types/lodash": "^4.14.118",
        "@types/node": "^6.14.2",
        "codelyzer": "^4.5.0",
        "cpx": "^1.5.0",
        "hammerjs": "^2.0.8",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^3.1.3",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^1.4.3",
        "karma-jasmine": "^1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "material-design-icons": "^3.0.1",
        "mdi": "^2.2.43",
        "protractor": "^5.4.1",
        "replace-in-file": "^3.4.2",
        "sass": "^1.15.2",
        "ts-node": "~4.1.0",
        "tslint": "~5.9.1",
        "typescript": "^2.9.2",
        "webpack": "^3.12.0",
        "webpack-dev-server": "^3.1.10"
    }
}
